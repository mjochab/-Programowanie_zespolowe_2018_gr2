      <TableView fx:id="receiptTable" layoutX="25.0" layoutY="6.0" prefHeight="200.0" prefWidth="492.0">
        <columns>
          <TableColumn fx:id="colName" prefWidth="95.0" text="Imie" />
          <TableColumn fx:id="colLastName" prefWidth="95.0" text="Nazwisko" />
            <TableColumn fx:id="colDesc" prefWidth="204.0" text="Opis naprawy" />
            <TableColumn fx:id="colCost" minWidth="6.0" prefWidth="97.0" text="Koszt naprawy" />
